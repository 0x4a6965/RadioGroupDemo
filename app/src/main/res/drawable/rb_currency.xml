<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:state_checked="true">
		<shape android:shape="rectangle">
			<solid android:color="@color/rb_select"/>
			<padding android:bottom="@dimen/dimen2" android:left="@dimen/dimen2" android:right="@dimen/dimen2" android:top="@dimen/dimen2"/>
			<corners android:radius="@dimen/dimen2"/>
		</shape>
	</item>

	<item>
		<shape android:shape="rectangle">
			<solid android:color="@color/white"/>
			<stroke android:width="@dimen/dimen1" android:color="@color/rb_select"/>
			<padding android:bottom="@dimen/dimen2" android:left="@dimen/dimen2" android:right="@dimen/dimen2" android:top="@dimen/dimen2"/>
			<corners android:radius="@dimen/dimen2"/>
		</shape>
	</item>
</selector>
